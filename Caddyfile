:80 {
    root /var/www/public
    log stdout
    gzip

    redir 301 {
        if {>X-Forwarded-Proto} is http
        /  https://{host}{uri}
    }

    redir /post/our-master-plan/ /post/our-roadmap/

    ext .html

    cors / {
        origin            http://projector.tensorflow.org
        methods           GET
    }

    errors {
        404 404.html
    }
}
